{
    "name": "cyrixbiz/acl",
	"description": "An Access Control List for Laravel without Gates",
    "keywords": [
        "auth",
        "permissions",
        "roles",
        "security",
        "acl",
        "middleware"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Kupka",
            "email": "admin@cyrix.biz",
            "homepage": "http://www.cyrix.biz",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">= 7.3",
        "doctrine/dbal": ">=2.13.2",
		"laravel/ui": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "cyrixbiz\\acl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cyrixbiz\\acl\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "cyrixbiz\\acl\\AclServiceProvider"
            ]
        }
    }
}
